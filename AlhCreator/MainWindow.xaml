<Window x:Class="AlhCreator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlhCreator"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <Grid Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
        <Canvas x:Name="canvas1" Background="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}" HorizontalAlignment="Left" VerticalAlignment="Top">
            
        </Canvas>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="33,25,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
    </Grid>
    

</Window>
